#include <bits/stdc++.h>
#define lli long long int
using namespace std;
 
int main(){
    int t;
    cin>>t;
    int ans=0;
    while(t--){
        string s;
        cin>>s;
        if(s[0]=='+' && s[1]=='+' || s[1]=='+' && s[2]=='+'){
            ans++;
        }
        else if(s[0]=='-' && s[1]=='-' || s[1]=='-' && s[2]=='-'){
            ans--;
        }
    }
    cout<<ans<<endl;
}
 
